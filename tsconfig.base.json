{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@dream/ui/*": ["libs/ui/src/lib/*"],
      "@dream/prisma": ["libs/prisma/src/index.ts"],
      "@dream/containers/*": ["libs/containers/src/*"],
      "@dream/utils/*": ["libs/utils/src/*"],
      "@dream/chat": ["libs/chat/src/index.ts"],
      "@dream/community": ["libs/community/src/index.ts"],
      "@dream/auth-api": ["libs/auth-api/src/index.ts"],
      "@dream/user-api": ["libs/user-api/src/index.ts"],
      "@dream/connection-api": ["libs/connection-api/src/index.ts"],
      "@dream/pepega-ui/*": ["libs/pepega-ui/src/lib/*"],
      "@dream/components/*": ["libs/components/src/*"],
      "@dream/icons/*": ["libs/icons/src/*"],
      "@dream/layouts/*": ["libs/layouts/src/*"],
      "@dream/auth": ["libs/auth/src/index.ts"],
      "@dream/spotify-now": ["libs/spotify-now/src/index.ts"],
      "@dream/user": ["libs/user/src/index.ts"],
      "@dream/community-api": ["libs/community-api/src/index.ts"],
      "@dream/mode-waitlist-api": ["libs/mode-waitlist-api/src/index.ts"],
      "@dream/mode-waitlist": ["libs/mode-waitlist/src/index.ts"],
      "@dream/types": ["libs/types/src/index.tsx"],
      "@dream/spotify-api": ["libs/spotify-api/src/index.ts"],
      "@dream/mods/waitlist-spotify/api": [
        "libs/mods/waitlist-spotify/api/src/index.ts"
      ],
      "@dream/mods/waitlist-spotify/ui": [
        "libs/mods/waitlist-spotify/ui/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
