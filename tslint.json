{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "no-implicit-dependencies": [true, "dev"],
    "prettier": true,
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "ban-comma-operator": false,
    "jsx-boolean-value": [
      true,
      "never"
    ]
  }
}
